<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link type="text/css" rel="stylesheet" href="~/Content/AdminLayout.css" />
    @if (ViewBag.ViewName == "AdminIndex")
    {
        <link type="text/css" rel="stylesheet" href="~/Content/Admin/AdminIndex.css" />
    }
    @if (ViewBag.ViewName == "AdminAddSet")
    {
        <link type="text/css" rel="stylesheet" href="~/Content/Admin/AdminAddSet.css" />
        <link rel="stylesheet" href="~/Content/font-awesome.min.css">
    }

    <script src='@Url.Content("~/Scripts/jquery-3.3.1.min.js")' type="text/javascript"></script>

</head>
<body>
    <div class="body-content">
        @RenderBody()
        <hr />

        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application sd  efwer werqw   sasdasd  asdfasfa f awqrqwr  </p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

